/* pages/Movies/Movies.wxss */
.maincontainer{
font-family: "Microsoft YaHei";
}
.bg{
  width:  100%;
  height: 300rpx;
  background-image:url("data:image/jpeg;base64,");
  background-size: 90% 100%;
  background-repeat: no-repeat;
  background-position: center;
  text-align: center;
}
.logo{
  padding-top: 100rpx;
  color: #FFFFFF;
}
.first{
  font-size: 48rpx;
  font-weight: bold;
}
.second{
  font-size: 28rpx;
}
.hr{
  background: rgb(255, 255, 255);
  height: 60rpx;
}
.menu{
  text-align: center;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.hotmovie{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.movie{
  display: flex;
  flex-direction: row;
  width: 95%;
  margin: 0 auto;
  border: 2rpx solid rgb(204, 204, 204);
  box-shadow: -6rpx 0rpx 18rpx #ECECEC,0px -6rpx 18rpx #ECECEC,0rpx 0rpx 18rpx #ECECEC,0rpx 0rpx 18rpx #ECECEC;
  margin-bottom: 20rpx;
  align-items: center;
}
.moviepic{
  width: 100rpx;
  height: 160rpx;
  padding: 20rpx;
}
.movieinformation{
  margin-left: 40rpx;
  margin-right: 20rpx;
}
.baseinformation{
  width: 300rpx;
  font-size: 30rpx;
  padding-top: 20rpx;
  line-height: 40rpx;
}
.title{
  font-size: 30rpx;
  font-weight: bold;
  color: #000000;
}
.people{
  color: #555555;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
.director{
  font-size: 20rpx;
  color: #E53D30;
}
.main_actor{
  font-size: 20rpx;
}
.time{
  font-size: 20rpx;
}
.type{
  font-size: 20rpx;
}
.rate{
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.text{
  width:  120rpx;
  height: 200rpx;
}
.tag{
  font-size: 30rpx;
}
.value{
  font-size: 60rpx;
  font-weight: bold;
}
.content {
  margin: 0 auto;
  font-size: 20rpx;
  /*关键属性（必须有的）  */
  display: -webkit-box;
  /*规定子元素的排列方向 */
  -webkit-box-orient: vertical;
  /* 当溢出是隐藏 */
  overflow: hidden;
  text-overflow: ellipsis;
}
 
.hide {
  -webkit-line-clamp: 2;
  /* 透明度,半透明 */
  opacity: 0.75;
}
 
.show {
  -webkit-line-clamp: 0;
  opacity: 1;
}
 
.state {
  color: black;
  float: right;
  font-size: 20rpx;
  margin-right: 30rpx;
}

