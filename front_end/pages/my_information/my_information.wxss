/* pages/My Information/My Information.wxss */
.maincontainer{
font-family: "楷体";
}
.bg{
  width:  100%;
  height: 400rpx;
  background-image: url("data:image/jpeg;base64,");
  background-size: 100% 100%;
  text-align: center;
}
.logo{
  padding-top: 20rpx;
  color: #0000FF;
}
.first{
  font-size: 48rpx;
  font-weight: bold;
}
.second{
  font-size: 28rpx;
}
.hr{
  background: rgb(255, 255, 255);
  height: 60rpx;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {

  color: rgb(14, 168, 230);
}

.hothistory{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.history{
  display: flex;
  flex-direction: row;
  width: 95%;
  margin: 0 auto;
  border: 2rpx solid rgb(204, 204, 204);
  box-shadow: -6rpx 0rpx 18rpx #ECECEC,0px -6rpx 18rpx #ECECEC,0rpx 0rpx 18rpx #ECECEC,0rpx 0rpx 18rpx #ECECEC;
  margin-bottom: 20rpx;
  align-items: center;
}
.historypic{
  width: 100rpx;
  height: 160rpx;
  padding: 20rpx;
}
.historyinformation{
  margin-left: 40rpx;
  margin-right: 20rpx;
}
.baseinformation{
  width: 300rpx;
  font-size: 30rpx;
  padding-top: 20rpx;
  line-height: 40rpx;
}
.title{
  font-size: 130rpx;
  font-weight: bold;
  color: #000000;
}
.people{
  color: #555555;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
.director{
  font-size: 20rpx;
  color: #E53D30;
}
.main_actor{
  font-size: 20rpx;
}
.time{
  font-size: 20rpx;
}
.type{
  font-size: 20rpx;
}
.rate{
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.text{
  width:  120rpx;
  height: 200rpx;
}
.tag{
  font-size: 30rpx;
}
.value{
  font-size: 60rpx;
  font-weight: bold;
}

